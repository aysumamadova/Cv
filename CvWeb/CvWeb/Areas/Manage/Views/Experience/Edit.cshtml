@model Experience
@{
    ViewData["Title"] = "Edit";
}

<div class="container-fluid">
    <form method="post" enctype="multipart/form-data">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="Title1">Title1</label>
                <input asp-for="Title1" class="form-control" placeholder="Title1">
            </div>
            <div class="form-group col-md-6">
                <label asp-for="Title2">Title2</label>
                <input asp-for="Title2" class="form-control" placeholder="Title2">
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-4">
                <label asp-for="Desc">Description</label>
                <input asp-for="Desc" class="form-control" placeholder="Description">
            </div>
            <div class="form-group col-md-4">
                <label asp-for="History">History</label>
                <input asp-for="History" class="form-control" placeholder="History">
            </div>

        </div>
        <div class="col-lg-5">
            <div class="form-group">
                <button type="submit" class="btn btn-primary">Edit</button>
            </div>
        </div>
    </form>
</div>
